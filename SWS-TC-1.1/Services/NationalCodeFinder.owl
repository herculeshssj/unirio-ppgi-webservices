<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF 
 xmlns:owl = "http://www.w3.org/2002/07/owl#"
 xmlns:rdfs = "http://www.w3.org/2000/01/rdf-schema#"
 xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:service = "http://www.daml.org/services/owl-s/1.1/Service.owl#"
 xmlns:process = "http://www.daml.org/services/owl-s/1.1/Process.owl#"
 xmlns:profile = "http://www.daml.org/services/owl-s/1.1/Profile.owl#"
 xmlns:grounding = "http://www.daml.org/services/owl-s/1.1/Grounding.owl#"
 xmlns:expr = "http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#"
 xmlns:swrl = "http://www.w3.org/2003/11/swrl#"
 xml:base = "http://127.0.0.1/services/NationalCodeFinder.owl"> 

<owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://127.0.0.1/ontology/Concepts.owl"/>
</owl:Ontology>

<service:Service rdf:ID="NationalCodeFinder">
    <service:presents rdf:resource="#NationalCodeFinder-Profile"/>
    <service:describedBy rdf:resource="#NationalCodeFinder-Process-Model"/>
    <service:supports rdf:resource="#NationalCodeFinder-Grounding"/>
</service:Service>

<profile:Profile rdf:ID="NationalCodeFinder-Profile">
    <service:isPresentedBy rdf:resource="#NationalCodeFinder-Service"/>
	 <profile:serviceName xml:lang="en">
           National Code Finder
    </profile:serviceName>
    <profile:textDescription xml:lang="en">
           Finds national code of a person
    </profile:textDescription>
    <profile:hasInput rdf:resource="#PersonInfo"/>
    <profile:hasOutput rdf:resource="#NationalCodeNumber"/>
</profile:Profile>

<process:ProcessModel rdf:ID="NationalCodeFinder-Process-Model">
     <service:describes rdf:resource="#NationalCodeFinder-Service"/>
     <process:hasProcess rdf:resource="#NationalCodeFinder-Process"/>
</process:ProcessModel>

<process:AtomicProcess rdf:ID="NationalCodeFinder-Process">
   <process:hasInput rdf:resource="#PersonInfo"/>
   <process:hasOutput rdf:resource="#NationalCodeNumber"/>
</process:AtomicProcess>

<process:Input rdf:ID="PersonInfo">
    <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/Concepts.owl#Person</process:parameterType>
    <rdfs:label>Person Information</rdfs:label>
</process:Input>

<process:Output rdf:ID="NationalCodeNumber">
    <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/Concepts.owl#NationalCode</process:parameterType>
    <rdfs:label>National Code Number of person</rdfs:label>
</process:Output>

<grounding:WsdlGrounding rdf:ID="NationalCodeFinder-Grounding">
    <service:supportedBy rdf:resource="#NationalCodeFinder-Service"/>
</grounding:WsdlGrounding>

</rdf:RDF>
