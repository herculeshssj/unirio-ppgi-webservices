wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-flight"
namespace { _"http://www.uniriotec.br/wsmo/services#"
, 
     dc _"http://purl.org/dc/elements/1.1/", 
     wsml _"http://www.wsmo.org/wsml/wsml-syntax#", 
     xsd _"http://www.w3.org/2001/XMLSchema#", 
     targetnamespace _"http://www.uniriotec.br/wsmo/services#" }


webService _"http://www.uniriotec.br/wsmo/services/GetWeatherInformation"
     nonFunctionalProperties
          _"http://purl.org/dc/elements/1.1#description" hasValue "City Weather "
          _"http://purl.org/dc/elements/1.1#title" hasValue "Web Service 1 3 2 "
     endNonFunctionalProperties

     importsOntology
            _"http://www.uniriotec.br/wsmo/ontology/Concepts.owl"

capability targetnamespace#GetWeatherInformationCapability

postcondition targetnamespace#GetWeatherInformationAxiom
     definedBy 
          ?a memberOf _"http://127.0.0.1/ontology/Concepts.owl#City" and ?b memberOf _"http://127.0.0.1/ontology/Concepts.owl#Weather".

